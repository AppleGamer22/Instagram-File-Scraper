{
	"name": "@applegamer22/social-scraper",
	"description": "Social media web scarper for post file downloads.",
	"version": "1.0.0-alpha.7",
	"author": "Omri Bornstein",
	"publisher": "AppleGamer22",
	"license": "MIT",
	"private": false,
	"repository": {
		"url": "https://github.com/AppleGamer22/Social-Scraper",
		"type": "git"
	},
	"keywords": [
		"nodejs",
		"typescript",
		"oclif",
		"cli",
		"chrome-headless",
		"puppeteer-core",
		"instagram-scraper",
		"vsco-scraper",
		"facebook-scraper"
	],
	"scripts": {
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"postpack": "rm -f oclif.manifest.json && rm -f tsconfig.tsbuildinfo",
		"posttest": "tslint -p test -t stylish",
		"prepack": "tsc && oclif-dev manifest && oclif-dev readme",
		"postpublish": "rm -rf lib/ && sudo npm i -g @applegamer22/social-scraper",
		"version": "oclif-dev readme && git add README.md"
	},
	"types": "lib/index.d.ts",
	"dependencies": {
		"@oclif/command": "^1.5.17",
		"@oclif/config": "^1.13.2",
		"@oclif/plugin-commands": "^1.2.2",
		"@oclif/plugin-help": "^2.2.0",
		"@oclif/plugin-not-found": "^1.2.2",
		"@oclif/plugin-warn-if-update-available": "^1.7.0",
		"@oclif/plugin-which": "^1.0.3",
		"cli-ux": "^5.3.1",
		"dotenv": "^8.0.0",
		"puppeteer-core": "^1.18.1",
		"tslib": "^1.10.0"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.22.0",
		"@oclif/test": "^1.2.4",
		"@oclif/tslint": "^3.1.1",
		"@types/chai": "^4.1.7",
		"@types/dotenv": "^6.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.6.2",
		"@types/puppeteer-core": "^1.9.0",
		"chai": "^4.2.0",
		"debug": "^4.1.1",
		"globby": "^10.0.1",
		"mocha": "^6.1.4",
		"nyc": "^14.1.1",
		"prettier": "^1.18.2",
		"ts-node": "^8.3.0",
		"tslint": "^5.18.0",
		"typescript": "^3.5.3"
	},
	"bin": {
		"sc": "./bin/run"
	},
	"files": [
		"/bin",
		"/lib",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "sc",
		"plugins": [
			"@oclif/plugin-commands",
			"@oclif/plugin-help",
			"@oclif/plugin-not-found",
			"@oclif/plugin-warn-if-update-available",
			"@oclif/plugin-which"
		]
	}
}
