{
	"name": "scrape",
	"description": "Social media web scarper for post file downloads.",
	"version": "1.0.0-alpha.1",
	"author": "AppleGamer22",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/AppleGamer22/Social-Scraper",
		"type": "git"
	},
	"scripts": {
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "tslint -p test -t stylish",
		"prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
		"test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif-dev readme && git add README.md"
	},
	"types": "lib/index.d.ts",
	"dependencies": {
		"@oclif/command": "^1.5.13",
		"@oclif/config": "^1.13.0",
		"@oclif/plugin-help": "^2.1.6",
		"cli-ux": "^5.2.1",
		"dotenv": "^8.0.0",
		"puppeteer": "^1.17.0",
		"tslib": "^1.9.3"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.22.0",
		"@oclif/test": "^1.2.4",
		"@oclif/tslint": "^3.1.1",
		"@types/chai": "^4.1.7",
		"@types/dotenv": "^6.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^12.0.0",
		"@types/puppeteer": "^1.12.4",
		"chai": "^4.2.0",
		"globby": "^9.2.0",
		"mocha": "^6.1.4",
		"nyc": "^14.1.1",
		"ts-node": "^8.1.0",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5"
	},
	"bin": {
		"scrape": "./bin/run"
	},
	"files": [
		"/bin",
		"/lib",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "scrape",
		"plugins": [
			"@oclif/plugin-help"
		]
	}
}
