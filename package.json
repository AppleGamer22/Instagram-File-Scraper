{
	"name": "@applegamer22/scr-cli",
	"description": "Social media web scraper for post file downloads.",
	"version": "1.0.0-alpha.29.8",
	"author": "Omri Bornstein",
	"publisher": "AppleGamer22",
	"license": "GPL-3.0-only",
	"private": false,
	"repository": {
		"url": "https://github.com/AppleGamer22/scr-cli",
		"type": "git"
	},
	"keywords": [
		"nodejs",
		"typescript",
		"oclif",
		"cli",
		"headless-chrome",
		"puppeteer-core",
		"instagram-scraper",
		"vsco-scraper",
		"tiktok-scraper"
	],
	"scripts": {
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"postpack": "rm -f oclif.manifest.json && rm -f tsconfig.tsbuildinfo",
		"posttest": "tslint -p test -t stylish",
		"prepack": "tsc && oclif-dev manifest && oclif-dev readme",
		"postpublish": "rm -rf lib/",
		"version": "oclif-dev readme && git add README.md"
	},
	"types": "lib/index.d.ts",
	"dependencies": {
		"@oclif/command": "^1.8.0",
		"@oclif/config": "^1.17.0",
		"@oclif/plugin-autocomplete": "^0.3.0",
		"@oclif/plugin-commands": "^1.3.0",
		"@oclif/plugin-help": "^3.2.2",
		"@oclif/plugin-not-found": "^1.2.4",
		"@oclif/plugin-warn-if-update-available": "^1.7.0",
		"@oclif/plugin-which": "^1.0.3",
		"axios": "^0.21.1",
		"chalk": "^4.1.0",
		"cli-ux": "^5.5.1",
		"dotenv": "^8.2.0",
		"puppeteer-core": "^7.0.4",
		"tslib": "^2.0.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.26.0",
		"@oclif/test": "^1.2.8",
		"@oclif/tslint": "^3.1.1",
		"@types/chai": "^4.2.14",
		"@types/dotenv": "^8.2.0",
		"@types/mocha": "^8.2.0",
		"@types/node": "^14.14.25",
		"@typescript-eslint/eslint-plugin": "^4.15.0",
		"@typescript-eslint/parser": "^4.15.0",
		"chai": "^4.2.0",
		"debug": "^4.1.1",
		"eslint": "^7.19.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^31.6.1",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"globby": "^11.0.1",
		"mocha": "^8.2.1",
		"prettier": "^2.2.1",
		"ts-node": "^9.0.0",
		"tslint": "^6.1.3",
		"typescript": "~4.0.3"
	},
	"bin": {
		"scr": "./bin/run"
	},
	"files": [
		"/bin",
		"/lib",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "scr",
		"plugins": [
			"@oclif/plugin-commands",
			"@oclif/plugin-help",
			"@oclif/plugin-not-found",
			"@oclif/plugin-warn-if-update-available",
			"@oclif/plugin-which",
			"@oclif/plugin-autocomplete"
		]
	},
	"mocha": {
		"require": "ts-node/register",
		"watch-files": [
			"src/**/.ts"
		],
		"recursive": true,
		"reporter": "spec",
		"timeout": 5000
	}
}
