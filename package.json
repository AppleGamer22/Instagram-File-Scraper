{
	"name": "@applegamer22/scr-cli",
	"description": "Social media web scraper for post file downloads.",
	"version": "1.0.0-alpha.27.1",
	"author": "Omri Bornstein",
	"publisher": "AppleGamer22",
	"license": "MIT",
	"private": false,
	"repository": {
		"url": "https://github.com/AppleGamer22/scr-cli",
		"type": "git"
	},
	"keywords": [
		"nodejs",
		"typescript",
		"oclif",
		"cli",
		"chrome-headless",
		"headless-chrome",
		"puppeteer-core",
		"instagram-scraper",
		"vsco-scraper",
		"facebook-scraper"
	],
	"scripts": {
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"postpack": "rm -f oclif.manifest.json && rm -f tsconfig.tsbuildinfo",
		"posttest": "tslint -p test -t stylish",
		"prepack": "tsc && oclif-dev manifest && oclif-dev readme",
		"postpublish": "rm -rf lib/",
		"version": "oclif-dev readme && git add README.md"
	},
	"types": "lib/index.d.ts",
	"dependencies": {
		"@oclif/command": "^1.5.19",
		"@oclif/config": "^1.14.0",
		"@oclif/plugin-commands": "^1.2.3",
		"@oclif/plugin-help": "^2.2.3",
		"@oclif/plugin-not-found": "^1.2.3",
		"@oclif/plugin-warn-if-update-available": "^1.7.0",
		"@oclif/plugin-which": "^1.0.3",
		"chalk": "^4.0.0",
		"cli-ux": "^5.4.5",
		"dotenv": "^8.2.0",
		"puppeteer-core": "^2.0.0",
		"superagent": "^5.2.2",
		"tslib": "^1.11.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.22.2",
		"@oclif/test": "^1.2.5",
		"@oclif/tslint": "^3.1.1",
		"@types/chai": "^4.2.11",
		"@types/dotenv": "^8.2.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.9.3",
		"@types/puppeteer-core": "^2.0.0",
		"@types/superagent": "^4.1.7",
		"chai": "^4.2.0",
		"debug": "^4.1.1",
		"globby": "^11.0.0",
		"mocha": "^7.1.1",
		"nyc": "^15.0.1",
		"prettier": "^2.0.2",
		"ts-node": "^8.8.2",
		"tslint": "^6.1.1",
		"typescript": "^3.8.3"
	},
	"bin": {
		"scr": "./bin/run"
	},
	"files": [
		"/bin",
		"/lib",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "scr",
		"plugins": [
			"@oclif/plugin-commands",
			"@oclif/plugin-help",
			"@oclif/plugin-not-found",
			"@oclif/plugin-warn-if-update-available",
			"@oclif/plugin-which"
		]
	},
	"mocha": {
		"require": "ts-node/register",
		"watch-files": [
			"src/**/.ts"
		],
		"recursive": true,
		"reporter": "spec",
		"timeout": 5000
	}
}
